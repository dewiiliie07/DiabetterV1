    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="com.android.launcher.permission.INSTALL_SHORTCUT" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">

        <service android:enabled="true" android:name=".Service.ServiceQuest">
        </service>
        <activity android:name=".editFoodActivity"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MenuUtama" />
        <activity android:name=".Register" />
        <activity android:name=".Menu_makanan" />

        <meta-data
            android:name="android.support.VERSION"
            android:value="26.0.1"
            tools:replace="android:value" />

        <activity android:name=".SettingActivity" />
        <activity android:name=".Birthdate" />
        <activity android:name=".Tall" />
        <activity android:name=".Weight" />
        <activity android:name=".Gender" />
        <activity android:name=".JenisAktivitas" />
        <activity android:name=".TermCondition" />
